# Supabase
SUPABASE_URL=
SUPABASE_ANON_KEY=

#speed
MIN_DOWNLOAD_MBPS=25
MIN_UPLOAD_MBPS=10
MAX_PING_MS=150

# Enable/disable alerts
ALERTS_ENABLED=true

# Alert cooldown (milliseconds) - prevents spam
ALERT_COOLDOWN_MS=300000

# Channel toggles
ALERT_CONSOLE=true
ALERT_LOG=true
ALERT_WEBHOOK=false
ALERT_EMAIL=false

# Webhook configuration
WEBHOOK_URL=https://hooks.slack.com/services/...
WEBHOOK_TIMEOUT=5000
WEBHOOK_RETRIES=3

# Email configuration
EMAIL_RECIPIENTS=admin@company.com,ops@company.com
EMAIL_FROM=noreply@company.com
EMAIL_SERVICE_URL=https://api.emailservice.com/send